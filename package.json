{
  "name": "collection-taro",
  "version": "1.0.0",
  "private": true,
  "description": "简约博客小程序",
  "templateInfo": {
    "name": "vue3",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "NODE_ENV=development taro build --type weapp",
    "build:weapp:prod": "NODE_ENV=production taro build --type weapp",
    "build:swan": "NODE_ENV=development taro build --type swan",
    "build:alipay": "NODE_ENV=development taro build --type alipay",
    "build:tt": "NODE_ENV=development taro build --type tt",
    "build:h5": "NODE_ENV=development taro build --type h5",
    "build:h5:prod": "NODE_ENV=production taro build --type h5",
    "build:rn": "NODE_ENV=development taro build --type rn",
    "build:qq": "NODE_ENV=development taro build --type qq",
    "build:jd": "NODE_ENV=development taro build --type jd",
    "build:quickapp": "NODE_ENV=development taro build --type quickapp",
    "dev:weapp": "NODE_ENV=development npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.6.25",
    "@tarojs/helper": "3.6.25",
    "@tarojs/plugin-platform-weapp": "3.6.25",
    "@tarojs/plugin-platform-alipay": "3.6.25",
    "@tarojs/plugin-platform-tt": "3.6.25",
    "@tarojs/plugin-platform-swan": "3.6.25",
    "@tarojs/plugin-platform-jd": "3.6.25",
    "@tarojs/plugin-platform-qq": "3.6.25",
    "@tarojs/plugin-platform-h5": "3.6.25",
    "@tarojs/runtime": "3.6.25",
    "@tarojs/shared": "3.6.25",
    "@tarojs/taro": "3.6.25",
    "@tarojs/plugin-framework-vue3": "3.6.25",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "3.6.25",
    "@tarojs/webpack5-runner": "3.6.25",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.3.0",
    "babel-preset-taro": "3.6.25",
    "eslint": "^8.12.0",
    "eslint-config-taro": "3.6.25",
    "eslint-plugin-vue": "^8.0.0",
    "eslint-plugin-import": "^2.12.0",
